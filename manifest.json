{
  "manifest_version": 2,

  "name": "TwitchTheater.tv Styles",
  "description": "1440x900 style for https://twitchtheater.tv/",
  "version": "0.2.5",
  "icons": {
    "192": "images/favicon-192.png"
  },
  "content_scripts": [{
    "matches": [ "*://*.twitchtheater.tv/*", "*://*.darkchilliz.github.io/*" ],
    "run_at": "document_idle",
    "js": ["js/content.js"]
  }],
  "web_accessible_resources": [
    "css/main.css",
    "js/main.js",
    "images/functionsmenu.png",
    "images/playerstyle.png"
  ],
  "homepage_url": "https://github.com/DarkChilliz/chrome-extension-twitchtheater.tv"
}
