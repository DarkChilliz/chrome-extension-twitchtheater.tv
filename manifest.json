{
  "manifest_version": 2,

  "name": "TwitchTheater.tv Styles",
  "description": "1440x900 style for https://twitchtheater.tv/",
  "version": "0.2.3",
  "icons": {
    "192": "favicon-192.png"
  },
  "content_scripts": [{
    "js": ["content.js"],
    "run_at": "document_end",
    "matches": ["*://*.twitchtheater.tv/*"]
  }],
  "web_accessible_resources": [
    "main.js",
    "menu.png"
  ],
  "homepage_url": "https://github.com/DarkChilliz/chrome-extension-twitchtheater.tv"
}
